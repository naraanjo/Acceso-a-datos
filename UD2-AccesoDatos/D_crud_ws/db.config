# Configuración de la aplicación para la base de datos SQLite
# Driver JDBC de la base de datos (clase del driver)
driver=org.sqlite.JDBC
# URL JDBC a la base de datos SQLite (relativa a la raíz del proyecto o absoluta)
url=jdbc:sqlite:usuarios.db
# SQLite no requiere usuario/contraseña, pero se incluyen por compatibilidad
user=
password=